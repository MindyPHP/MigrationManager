{
    "name": "mindy/migration-bundle",
    "license": "MIT",
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "conflict": {
        "doctrine/doctrine-bundle": "*"
    },
    "autoload": {
        "psr-4": {
            "Mindy\\Component\\MigrationManager\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mindy\\Component\\MigrationManager\\Tests\\": "./Tests/"
        }
    },
    "require": {
        "php" : ">=7.0",
        "doctrine/dbal": "~2.0",
        "doctrine/migrations": "~1.0"
    },
    "authors": [
        {
            "name": "Maxim Falaleev",
            "email": "max@studio107.ru",
            "homepage": "http://studio107.ru"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "symfony/dependency-injection": "~3.0",
        "symfony/filesystem": "~3.0",
        "symfony/http-kernel": "~3.0"
    }
}
